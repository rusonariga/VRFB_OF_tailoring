FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      porosityProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

porous
{
    type            DarcyForchheimer;
    active          yes;
    cellZone        porous;

    DarcyForchheimerCoeffs
    {
        d   (3.7e10     3.7e10      3.7e10);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);

            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    
                e2      (0 1 0);
            }
        }
    }
}
