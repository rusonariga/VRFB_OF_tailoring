FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293;

boundaryField
{
    wall_porous
    {
        type            zeroGradient;
    }
    wall_channel
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 293;
        value           uniform 293;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 293;
    }
}


