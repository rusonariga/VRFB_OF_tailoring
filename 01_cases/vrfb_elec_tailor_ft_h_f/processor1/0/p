FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls_channel
    {
        type            zeroGradient;
    }
    walls_porous
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    walls_intrusion
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0;
    }
}


